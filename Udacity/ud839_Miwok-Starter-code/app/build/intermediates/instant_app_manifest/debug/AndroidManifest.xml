<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.miwok"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.android.miwok.PhrasesActivity"
            android:label="@string/category_phrases_label"
            android:parentActivityName="com.example.android.miwok.MainActivity" />
        <activity
            android:name="com.example.android.miwok.ColorsActivity"
            android:label="@string/category_colors_label"
            android:parentActivityName="com.example.android.miwok.MainActivity" />
        <activity
            android:name="com.example.android.miwok.FamilyMembersActivity"
            android:label="@string/category_familyMembers_label"
            android:parentActivityName="com.example.android.miwok.MainActivity" />
        <activity
            android:name="com.example.android.miwok.NumbersActivity"
            android:label="@string/category_numbers_label"
            android:parentActivityName="com.example.android.miwok.MainActivity" />
        <activity android:name="com.example.android.miwok.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>